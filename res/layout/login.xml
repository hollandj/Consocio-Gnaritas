<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
	android:id="@+id/widget29"
	android:layout_width="fill_parent" 
	android:layout_height="fill_parent"
	android:orientation="vertical" 
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="@color/white" android:alwaysDrawnWithCache="false">
	<LinearLayout 
		android:id="@+id/widget63"
		android:layout_width="fill_parent" 
		android:orientation="vertical"
		android:layout_height="245dip">
		<LinearLayout 
			android:id="@+id/linearLayout1" 
			android:layout_width="fill_parent" 
			android:layout_height="fill_parent" 
			android:orientation="vertical">
			<ImageView 
				android:layout_gravity="center"
				android:background="@drawable/coagmentologo" 
				android:id="@+id/imageView1"
				android:layout_marginTop="50dip" 
				android:layout_height="125dip" 
				android:layout_width="125dip"></ImageView>
			<ImageView 
				android:layout_marginRight="50dip" 
				android:id="@+id/imageView2" 
				android:background="@drawable/coagmento" 
				android:layout_marginLeft="35dip" 
				android:layout_width="245dip" 
				android:layout_height="100dip"></ImageView>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout 
		android:id="@+id/widget64"
		android:layout_height="wrap_content" 
		android:orientation="vertical"
		android:layout_width="fill_parent" 
		android:layout_marginTop="34dip">
		<EditText
		 android:layout_gravity="center_horizontal"
			android:hint="Username" 
			android:focusable="true" 
			android:imeOptions="actionNext"
			android:inputType="text" 
			android:layout_width="225dip"
			android:id="@+id/usernameInput" 
			android:layout_height="35dip" 
			android:textSize="14sp">
		</EditText>
		<EditText android:inputType="textPassword"
			android:imeOptions="actionDone" 
			android:focusable="true"
			android:layout_gravity="center_horizontal|center" 
			android:hint="Password"
			android:layout_width="225dip" 
			android:id="@+id/passwordInput"
			android:layout_marginTop="5dip" 
			android:layout_height="35dip" 
			android:textSize="14sp">
		</EditText>
		<Button android:text="Login" 
			android:focusable="true"
			android:layout_gravity="center_horizontal" 
			android:layout_width="170dip"
			android:id="@+id/LoginButton" 
			android:layout_height="35dip" 
			android:layout_marginTop="10dip" android:textSize="12sp">
		</Button>
	</LinearLayout>
	<LinearLayout 
		android:id="@+id/widget66"
		android:layout_height="wrap_content" 
		android:orientation="horizontal" 
		android:layout_marginTop="10dip" 
		android:layout_width="fill_parent" 
		android:weightSum="1">
		<RelativeLayout 
			android:id="@+id/relativeLayout1" 
			android:layout_height="wrap_content" 
			android:layout_width="fill_parent" 
			android:layout_marginTop="10dip">
			<CheckBox 
				android:id="@+id/RememberLogin" 
				android:layout_height="wrap_content" 
				android:layout_width="150dip" 
				android:textSize="14sp" 
				android:checked="true" 
				android:text="Remember Me" 
				android:textColor="@color/black" 
				android:layout_marginLeft="11dip"></CheckBox>
			<TextView 
				android:id="@+id/registerClick" 
				android:layout_width="75dip" 
				android:clickable="true" 
				android:textColor="@color/black" 
				android:layout_height="wrap_content" 
				android:text="Register &gt;&gt;" 
				android:layout_centerVertical="true" 
				android:layout_alignParentRight="true" 
				android:layout_marginRight="29dp" 
				android:textSize="14sp"></TextView>
		</RelativeLayout>
	</LinearLayout>
</LinearLayout>