<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
	android:id="@+id/widget29"
	android:layout_width="fill_parent" 
	android:layout_height="fill_parent"
	android:orientation="vertical" 
	xmlns:android="http://schemas.android.com/apk/res/android" 
	android:background="@color/white">
	<LinearLayout 
		android:id="@+id/widget63"
		android:layout_width="fill_parent" 
		android:orientation="vertical" 
		android:layout_height="110dip">
		<LinearLayout 
			android:id="@+id/linearLayout1" 
			android:layout_width="fill_parent" 
			android:layout_height="fill_parent" 
			android:orientation="horizontal">
			<ImageView 
				android:layout_gravity="center" 
				android:background="@drawable/coagmentologo" 
				android:id="@+id/imageView1" 
				android:layout_marginLeft="75dip" 
				android:layout_height="100dip" 
				android:layout_width="100dip"></ImageView>
			<ImageView 
				android:background="@drawable/coagmento" 
				android:layout_marginRight="50dip" 
				android:id="@+id/imageView2" 
				android:layout_width="215dip" 
				android:layout_marginLeft="30dip" 
				android:layout_height="wrap_content" 
				android:layout_marginTop="5dip"></ImageView>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout 
		android:id="@+id/widget64"
		android:layout_height="wrap_content" 
		android:orientation="vertical"
		android:layout_width="fill_parent" 
		android:layout_marginTop="15dip">
		<EditText 
			android:layout_gravity="center_horizontal"
			android:hint="Username" 
			android:focusable="true" 
			android:imeOptions="actionNext" 
			android:inputType="text" 
			android:layout_width="225dip" 
			android:id="@+id/usernameInput" 
			android:layout_height="30dip" 
			android:textSize="12sp">
		</EditText>
		<EditText 
		android:inputType="textPassword" 
		android:imeOptions="actionDone" 
		android:focusable="true" 
		android:layout_gravity="center_horizontal|center"  
		android:hint="Password" 
		android:layout_width="225dip" 
		android:id="@+id/passwordInput" 
		android:layout_marginTop="5dip" 
		android:layout_height="30dip" 
		android:textSize="12sp">
		</EditText>
		<Button 
			android:text="Login" 
			android:focusable="true"
			android:layout_gravity="center_horizontal" 
			android:layout_width="170dip"
			android:id="@+id/LoginButton" 
			android:layout_marginTop="5dip" 
			android:layout_height="30dip" 
			android:textSize="8sp">
		</Button>
	</LinearLayout>
	<LinearLayout 
		android:id="@+id/widget66"
		android:orientation="horizontal" 
		android:layout_width="fill_parent" 
		android:layout_height="fill_parent">
		<RelativeLayout 
			android:id="@+id/relativeLayout1" 
			android:layout_width="fill_parent" 
			android:layout_height="fill_parent">
			<CheckBox 
				android:id="@+id/RememberLogin" 
				android:layout_width="wrap_content" 
				android:textSize="14sp" 
				android:checked="true" 
				android:text="Remember Me" 
				android:textColor="@color/black" 
				android:layout_height="wrap_content" 
				android:layout_marginLeft="45dip"></CheckBox>
			<TextView 
				android:id="@+id/registerClick"
				android:textSize="14.5sp" 
			 	android:clickable="true" 
			 	android:textColor="@color/black" 
			 	android:text="Register &gt;&gt;" 
			 	android:layout_centerVertical="true" 
			 	android:layout_alignParentRight="true" 
			 	android:layout_marginRight="60dp" 
			 	android:layout_height="wrap_content" 
			 	android:layout_width="100dip"></TextView>
		</RelativeLayout>
	</LinearLayout>
</LinearLayout>

